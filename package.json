    "html-webpack-plugin": "^3.2.0",
    "jest": "~23.4.2",
    "rimraf": "~2.6.2",
    "ts-jest": "~23.0.1",
    "tslint": "~5.11.0",
    "tslint-microsoft-contrib": "~5.1.0",
    "tsutils": "~3.0.0",
    "typescript": "~3.0.1",
    "webpack": "^4.16.3",
